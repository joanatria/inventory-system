<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="310.0" prefWidth="329.0" style="-fx-background-color: #ECE5DD;" stylesheets="@design.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mp_mijomi_cafe.EditIngredientController">
   <ImageView fitHeight="57.0" fitWidth="304.0" layoutX="24.0" layoutY="35.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@logo.png" />
      </image></ImageView>
    <GridPane layoutX="27.0" layoutY="100.0" prefHeight="153.0" prefWidth="276.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="132.0" minWidth="10.0" prefWidth="94.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="178.0" minWidth="10.0" prefWidth="178.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Label text="Add Amount" textFill="#594a47" wrapText="true" GridPane.rowIndex="1">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label text="SKU" textFill="#594a47">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TextField fx:id="itemSizeField" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <font>
                <Font size="15.0" />
            </font>
        </TextField>
        <TextField fx:id="brandField" GridPane.columnIndex="1">
            <font>
                <Font size="15.0" />
            </font>
        </TextField>
    </GridPane>
    <ButtonBar layoutX="112.0" layoutY="254.0" prefHeight="46.0" prefWidth="146.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#okButtonClicked" text="OK">
                <font>
                    <Font name="AppleGothic Regular" size="13.0" />
                </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#cancelButtonClicked" prefWidth="77.0" text="Cancel">
                <font>
                    <Font name="AppleGothic Regular" size="13.0" />
                </font>
            </Button>
        </buttons>
    </ButtonBar>
</AnchorPane>
